{
  "name": "doors-smores",
  "publisher": "Niflheim",
  "displayName": "Doors Smores",
  "icon": "resources/smores.png",
  "description": "Requirements simplified",
  "sponsor": {
    "url": "https://www.buymeacoffee.com/crnsp74c9fp"
  },
  "repository": {
    "url": "https://github.com/Niflheim-uk/Doors-Smores",
    "type": "git"
  },
  "keywords": [
    "requirements",
    "documentation"
  ],
  "preview": true,
  "version": "0.6.5",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/doorsSmores.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "doors-smores.smoresEditor",
        "displayName": "Doors Smores",
        "selector": [
          {
            "filenamePattern": "*.smores"
          }
        ]
      },
      {
        "viewType": "doors-smores.smoresConverter",
        "displayName": "Doors Smores Project Converter",
        "selector": [
          {
            "filenamePattern": "*.smores-project"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.18.34",
    "@types/showdown": "^2.0.2",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.26.0",
    "showdown": "^2.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "fast-xml-parser": "^4.3.1"
  }
}
